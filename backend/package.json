{
  "name": "jgrantsv3-server",
  "private": true,
  "version": "0.0.0",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "type": "module",
  "scripts": {
    "local": "nodemon",
    "dev": "NODE_ENV=development tsx src/index.ts",
    "lint:filenames": "lint-file-names",
    "lint:check": "eslint --ext ts,tsx src/",
    "lint:pipelinecheck": "eslint --ext ts,tsx ",
    "lint:fix": "eslint --fix --ext ts,tsx src/",
    "format": "prettier --write \"src/**/(*.tsx|*.ts)\"",
    "format:check": "prettier --check \"src/**/(*.tsx|*.ts)\"",
    "format:pipelinecheck": "prettier --check "
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.2",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.44.0",
    "eslint-plugin-check-file": "^2.6.2",
    "eslint-plugin-jsdoc": "^46.5.1",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "prettier-config-nick": "^1.0.8",
    "prisma": "^4.16.1",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-http-context": "^1.2.4",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "winston": "^3.10.0"
  }
}
